{
  "name": "electricity-meters",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_via_ip": "ng serve --proxy-config proxy_via_ip.config.json --port 4600 -c development",
    "start_local": "ng serve --proxy-config proxy_via_localhost.config.json --port 4600 -c development --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prerender": "ng run electricity-meters:prerender",
    "lint": "tsc --noEmit && eslint . --quiet --fix",
    "lint:nofix": "tsc --noEmit && npx eslint .",
    "prettier": "npx prettier --write .",
    "lint-prettier": "npm run prettier && npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "angular2-notifications": "^16.0.1",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.3",
    "express": "^5.1.0",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.17.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.18",
    "@angular-eslint/eslint-plugin": "^17.5.3",
    "@angular-eslint/eslint-plugin-template": "^17.5.3",
    "@angular/cli": "~18.2.18",
    "@angular/compiler-cli": "^18.2.13",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.7",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "8.2.0",
    "angular-cli-ghpages": "^2.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.2",
    "typescript": "5.4.5"
